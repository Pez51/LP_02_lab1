public class Regalo {
  public static String eligir(int vida) {
    String gift = "";
    switch(vida) {
    case 1: gift = "Un pasaje al caribe"; break;
    case 2: gift = "Un visita al museao mas cercano a tu casa"; break;
    case 3: gift = "Una entrada al cine"; break;
    }
    return gift;
  }
}
